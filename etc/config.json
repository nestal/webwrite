{
 	"name"		: "WebWrite",
  	"socket"	: ":9000",
	"wb_root"	: "/webwrite",
	
	"lib"		: { "path": "lib",  	"redir": "/wb/lib" },
	"data"		: { "path": "data", 	"redir": "/wb/data" },
	"attic"		: { "path": "attic",	"redir": "/wb/attic" },
	"meta"		: { "path": "meta",		"redir": "/wb/meta" },
	
	"mime"		:
	{
		".js"	: "application/javascript",
		".pdf"	: "application/pdf",
		".css"	: "text/css",
		".html"	: "text/html",
		".jpg"	: "image/jpeg",
		".jpeg"	: "image/jpeg",
		".svg"	: "image/svg+xml",
		".png"	: "image/png",
	},
	"log"		:
	{
		"level"	: "debug",
		"file"	: "webwrite.log"
	}
}
